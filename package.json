{
  "name": "opensphere-jscodeshift",
  "version": "1.0.0",
  "description": "Scripts designed to migrate OpenSphere code toward ES6 modules.",
  "scripts": {
    "dryrun": "jscodeshift --dry --print",
    "init": "rimraf .build && mkdirp .build",
    "inspect": "node --inspect-brk ./node_modules/.bin/jscodeshift -d -p --run-in-band",
    "lint": "eslint --cache 'transforms/**/*.js' 'scripts/**/*.js'",
    "pretest": "npm run init && mkdirp .build/test && cp -rf test/*.input.js .build/test/ && jscodeshift -t scripts/alltransforms.js .build/test",
    "test": "nyc _mocha -- 'test/*.test.js'",
    "shift": "jscodeshift"
  },
  "author": "Kevin Schmidt",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "colors": "^1.3.0",
    "get-value": "^3.0.1",
    "jscodeshift": "^0.5.0"
  }
}
